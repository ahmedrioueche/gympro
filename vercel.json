{
  "buildCommand": "cd web && node scripts/setup-npmrc.js && npm install && npm run build",
  "outputDirectory": "web/dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*\\.(js|css|png|jpg|svg|woff|woff2|ttf|ico))",
      "destination": "/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
